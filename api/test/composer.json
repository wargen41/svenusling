{
    "name": "movie-api/slim-api",
    "version": "1.0.0",
    "description": "A movie database API built with Slim Framework",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.11",
        "slim/psr7": "^1.6",
        "firebase/php-jwt": "^6.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "audit": {
            "ignore": {
                "PKSA-y2cr-5h3j-g3ys": "Testar ju bara, så vi struntar i säkerhetshål just nu"
            }
        }
    }
}
